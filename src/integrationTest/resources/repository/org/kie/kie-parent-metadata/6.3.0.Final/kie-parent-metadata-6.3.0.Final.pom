    <version.org.jolokia>1.2.2</version.org.jolokia>
    <version.org.mortbay.jetty>6.1.25</version.org.mortbay.jetty>
    <version.org.mortbay.jetty.runner>8.1.7.v20120910</version.org.mortbay.jetty.runner>
    <version.org.apache.xmlbeans>2.3.0</version.org.apache.xmlbeans>
    <version.org.owasp.encoder>1.1</version.org.owasp.encoder>
    <version.org.drools.guvnor-api>5.6.0.Final</version.org.drools.guvnor-api>
    <version.org.eclipse.sisu>0.0.0.M5</version.org.eclipse.sisu>
    <!-- Dependency of maven 3.2.2  -->
    <version.org.codehaus.plexus.plexus-classworlds>2.5.1</version.org.codehaus.plexus.plexus-classworlds>
    <!-- Dependency of maven 3.2.2  -->
    <version.org.codehaus.plexus.plexus-interpolation>1.19</version.org.codehaus.plexus.plexus-interpolation>
    <!-- Dependency of maven 3.2.2  -->
    <version.org.sonatype.plexus.plexus-cipher>1.4</version.org.sonatype.plexus.plexus-cipher>
    <!-- Dependency of maven 3.2.2  -->
    <version.org.sonatype.plexus.plexus-sec-dispatcher>1.3</version.org.sonatype.plexus.plexus-sec-dispatcher>
    <!-- Dependency of maven 3.2.2  -->
    <version.org.sonatype.sisu.sisu-guice>3.1.0</version.org.sonatype.sisu.sisu-guice>
    <!-- Dependency of maven 3.2.2  -->
    <version.org.jboss.errai>3.2.0.Final</version.org.jboss.errai>
    <!-- Version of Errai compatible with CDI 1.0. Few artifacts in this version are used for CDI 1.0 compatible WAR distributions. -->
    <version.org.jboss.errai.cdi10-compatible>3.0.6.Final</version.org.jboss.errai.cdi10-compatible>
    <version.org.picketlink>2.6.0.Final</version.org.picketlink>
